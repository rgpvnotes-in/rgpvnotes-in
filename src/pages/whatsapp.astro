---
import Layout from '@layouts/Layout.astro';

const seoData = {
    pageTitle: 'Whatsapp redirection page',
    canonicalUrl: new URL(Astro.url.pathname, Astro.site),
    description: 'Whatsapp redirection page',
    keywords: 'Whatsapp redirection page',
    imageUrl: '',
}  
---

