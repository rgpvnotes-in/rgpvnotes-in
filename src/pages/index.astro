---
import Layout from '../layouts/Layout.astro';
import AboutComponent from '../components/homePage/aboutComponent/aboutComponent.vue';
import ContactComponent from '../components/homePage/contactComponent/contactComponent.vue';
import CountComponent from '../components/homePage/countComponent/countComponent.vue';
import FaqComponent from '../components/homePage/faqComponent/faqComponent.vue';
import FeaturesComponent from '../components/homePage/featuresComponent/featuresComponent.vue';
import HeroComponent from '../components/homePage/heroComponent/heroComponent.vue';
import PortfolioComponent from '../components/homePage/portfolioComponent/portfolioComponent.vue';
import PricingComponent from '../components/homePage/pricingComponent/pricingComponent.vue';
import ServicesComponent from '../components/homePage/servicesComponent/servicesComponent.vue';
import TestimonialsComponent from '../components/homePage/testimonialsComponent/testimonialsComponent.vue';

// Vendor CSS Files
import '@assets/vendor/aos/aos.css';
import '@assets/vendor/bootstrap-icons/bootstrap-icons.css';
import '@assets/vendor/glightbox/css/glightbox.min.css';
import '@assets/vendor/swiper/swiper-bundle.min.css';
---

<Layout title="" isInnerPage={false}>
    <HeroComponent client:load  />
    <main id="main">
        <AboutComponent  client:visible />
        <CountComponent  client:visible  />
        <ServicesComponent  client:visible  />
        <!-- <FeaturesComponent  client:visible  /> -->
        <!-- <TestimonialsComponent /> -->
        <PortfolioComponent  client:visible  />
        <!-- <PricingComponent  client:visible /> -->
        <!-- <FaqComponent  client:visible  /> -->
        <ContactComponent client:idle  />
    </main>

            <!-- Vendor JS Files -->  
            <script src="/assets/vendor/purecounter/purecounter_vanilla.js" is:global></script>
            <script src="/assets/vendor/aos/aos.js" is:global></script>
            <script src="/assets/vendor/glightbox/js/glightbox.min.js" is:global></script>
            <script src="/assets/vendor/isotope-layout/isotope.pkgd.min.js" is:global></script>
            <script src="/assets/vendor/swiper/swiper-bundle.min.js" is:global></script>
</Layout>

